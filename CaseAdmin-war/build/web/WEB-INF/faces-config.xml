<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>caseadminwar.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Page1</managed-bean-name>
        <managed-bean-class>caseadminwar.Page1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>caseadminwar.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>caseadminwar.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$Login</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.Login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$Register</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.Register</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>pages$MainMenu</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.MainMenu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/Login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>login_register</from-outcome>
            <to-view-id>/pages/Register.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/Register.jsp</from-view-id>
        <navigation-case>
            <from-outcome>register_login</from-outcome>
            <to-view-id>/pages/Login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/MainMenu.jsp</from-view-id>
        <navigation-case>
            <from-outcome>menu_login</from-outcome>
            <to-view-id>/pages/Login.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_viewCase</from-outcome>
            <to-view-id>/pages/ViewCase.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_viewUser</from-outcome>
            <to-view-id>/pages/ViewUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_editUser</from-outcome>
            <to-view-id>/pages/EditUser.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_editCase</from-outcome>
            <to-view-id>/pages/EditCase.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_addCase</from-outcome>
            <to-view-id>/pages/AddCase.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>menu_raffle</from-outcome>
            <to-view-id>/pages/Raffle.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ViewCase</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.ViewCase</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ViewCase.jsp</from-view-id>
        <navigation-case>
            <from-outcome>caseView_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$ViewUser</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.ViewUser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/ViewUser.jsp</from-view-id>
        <navigation-case>
            <from-outcome>userView_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$AddCase</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.AddCase</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/AddCase.jsp</from-view-id>
        <navigation-case>
            <from-outcome>addCase_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$EditCase</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.EditCase</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/EditCase.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editCase_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$EditUser</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.EditUser</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/EditUser.jsp</from-view-id>
        <navigation-case>
            <from-outcome>editUser_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>pages$Raffle</managed-bean-name>
        <managed-bean-class>caseadminwar.pages.Raffle</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/pages/Raffle.jsp</from-view-id>
        <navigation-case>
            <from-outcome>raffle_menu</from-outcome>
            <to-view-id>/pages/MainMenu.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
